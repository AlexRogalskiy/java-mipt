"  @Caption(\"user\")\n"
"  public User getUser(@Param(\"id\") String userId) {"

#: lecture.adoc:84
msgid ""
"    @SuppressWarnings(\"unchecked\")\n"
"    List<User> users = ....\n"
"  }\n"
"}"
msgstr ""
"    @SuppressWarnings(\"unchecked\")\n"
"    List<User> users = ....\n"
"  }\n"
"}"

#: lecture.adoc:90
msgid ""
"class Cache<@Immutable V> {\n"
" ...\n"
"}\n"
"----"
msgstr ""
"class Cache<@Immutable V> {\n"
" ...\n"
"}\n"
"----"

#: lecture.adoc:93
msgid "== Аннотирование пакетов -- в package-info.java"
