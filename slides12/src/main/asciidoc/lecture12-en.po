#: lecture.adoc:71
msgid "== Примеры использования аннотаций в декларациях"
msgstr "== Examples of the use of annotations in declarations"

#: lecture.adoc:76
msgid ""
"[source,java]\n"
"----\n"
"@Entity\n"
"public class User {"
msgstr ""
"[source,java]\n"
"----\n"
"@Entity\n"
"public class User {"

#: lecture.adoc:79
msgid ""
"  @Caption(\"user\")\n"
"  public User getUser(@Param(\"id\") String userId) {"
